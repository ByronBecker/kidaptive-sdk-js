{
  "name": "kidaptive-sdk-js",
  "version": "0.1.9",
  "description": "Kidaptive ALP Javascript SDK",
  "main": "./dist/kidaptive_sdk.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "install": "typings install",
    "watch": "watchify src/js/main/kidaptive_sdk.js -s KidaptiveSdk -o dist/kidaptive_sdk.js -v",
    "build:irt": "cd src/js/main/irt && (make clean; make)",
    "build:debug": "browserify src/js/main/kidaptive_sdk.js -s KidaptiveSdk -d -o dist/kidaptive_sdk.js",
    "build:prod": "browserify src/js/main/kidaptive_sdk.js -s KidaptiveSdk | uglifyjs > dist/kidaptive_sdk.min.js",
    "build": "npm run build:irt && (npm run build:debug; npm run build:prod)",
    "version": "npm run build && git add -A dist",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kidapt/kidaptive-sdk-js.git"
  },
  "keywords": [
    "kidaptive",
    "javascript",
    "alp",
    "sdk"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Kidapt/kidaptive-sdk-js/issues"
  },
  "homepage": "https://github.com/Kidapt/kidaptive-sdk-js#readme",
  "devDependencies": {
    "browserify": "^14.3.0",
    "uglify-js": "^3.0.11",
    "watchify": "^3.9.0"
  }
}
